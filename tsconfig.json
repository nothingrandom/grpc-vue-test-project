{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "target": "es6",
    "removeComments": true,
    "noImplicitReturns": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "lib": ["dom", "es2017"],
  },
}
